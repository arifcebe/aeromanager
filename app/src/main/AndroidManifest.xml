<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gac.aeromanager">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/Theme.Aeromanager">
        <activity android:name=".SplashActivity"
            android:theme="@style/Theme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".team.activity.TeamActivity"
            android:parentActivityName=".MainActivity">
            </activity>

        <activity android:name=".atlet.activity.AtletActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".round.activity.RoundActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".hasilakhir.activity.HasilAkhirDetailActivity"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>
